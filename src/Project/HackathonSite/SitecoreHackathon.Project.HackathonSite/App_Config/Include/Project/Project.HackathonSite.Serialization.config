<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<unicorn>
			<configurations>
				<configuration name="Project.HackathonSite" description="HackathonSite" extends="SitecoreHackathon.Project"  dependencies="Foundation.*,Feature.*,Project.Common">
					<predicate>
						<include name="Renderings" database="master" path="/sitecore/layout/Renderings/$(layer)/Hackathon Site"></include>
						<include name="Layout" database="master" path="/sitecore/layout/Layouts/$(layer)/Hackathon Site"></include>
					</predicate>
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>